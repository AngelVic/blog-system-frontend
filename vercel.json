{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npx pnpm install --registry=https://registry.npmjs.org/ && npx pnpm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install -g pnpm",
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
